{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "lib": ["es2016", "esnext.asynciterable"],
        "esModuleInterop": true,
        // "allowSyntheticDefaultImports": true
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "strict": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true
        // "noUnusedParameters": true,
    }
}
